{
  "semi": false,
  "singleQuote": true,
  "arrowParens": "always",
  "bracketSpacing": false,
  "endOfLine": "auto",
  "jsxBracketSameLine ": false,
  "printWidth": 200,
  "proseWrap": "preserve",
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.ejs",
      "options": {
        "parser": "html",
        "tabWidth": 2,
        "jsxBracketSameLine": true,
        "bracketSpacing": true
      }
    },
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
